{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df-demo-adls"
		},
		"Road_Weather_Demonstration_Data_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Road_Weather_Demonstration_Data'"
		},
		"adls_Autism_prevalence_studies_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'adls_Autism_prevalence_studies'"
		},
		"adls_National_Immunization_Survey_Adult_COVID_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'adls_National_Immunization_Survey_Adult_COVID'"
		},
		"adls_Viral_hemorrhagic_fevers_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'adls_Viral_hemorrhagic_fevers'"
		},
		"Dot_Road_Weather_Demonstration_Data_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://data.transportation.gov/api/views/jc5e-psbx/rows.json?accessType=DOWNLOAD"
		},
		"National_Immunization_Survey_Adult_COVID_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://data.cdc.gov/api/views/akkj-j5ru/rows.json?accessType=DOWNLOAD"
		},
		"Road_Weather_Demonstration_Data_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://brdohertdemoadlsg2.dfs.core.windows.net"
		},
		"Viral_hemorrhagic_fevers_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://data.cdc.gov/api/views/5nfe-wsjn/rows.json?accessType=DOWNLOAD"
		},
		"adls_Autism_prevalence_studies_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://brdohertdemoadlsg2.dfs.core.windows.net"
		},
		"adls_National_Immunization_Survey_Adult_COVID_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://brdohertdemoadlsg2.dfs.core.windows.net"
		},
		"adls_Viral_hemorrhagic_fevers_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://brdohertdemoadlsg2.dfs.core.windows.net"
		},
		"http_Autism_prevalence_studies_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://data.cdc.gov/api/views/9mw4-6adp/rows.json?accessType=DOWNLOAD"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "df-demo-adls_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Road_Weather_Demonstration_Data_accountKey": {
						"value": "[parameters('Road_Weather_Demonstration_Data_accountKey')]"
					},
					"adls_Autism_prevalence_studies_accountKey": {
						"value": "[parameters('adls_Autism_prevalence_studies_accountKey')]"
					},
					"adls_National_Immunization_Survey_Adult_COVID_accountKey": {
						"value": "[parameters('adls_National_Immunization_Survey_Adult_COVID_accountKey')]"
					},
					"adls_Viral_hemorrhagic_fevers_accountKey": {
						"value": "[parameters('adls_Viral_hemorrhagic_fevers_accountKey')]"
					},
					"Dot_Road_Weather_Demonstration_Data_properties_typeProperties_url": {
						"value": "[parameters('Dot_Road_Weather_Demonstration_Data_properties_typeProperties_url')]"
					},
					"National_Immunization_Survey_Adult_COVID_properties_typeProperties_url": {
						"value": "[parameters('National_Immunization_Survey_Adult_COVID_properties_typeProperties_url')]"
					},
					"Road_Weather_Demonstration_Data_properties_typeProperties_url": {
						"value": "[parameters('Road_Weather_Demonstration_Data_properties_typeProperties_url')]"
					},
					"Viral_hemorrhagic_fevers_properties_typeProperties_url": {
						"value": "[parameters('Viral_hemorrhagic_fevers_properties_typeProperties_url')]"
					},
					"adls_Autism_prevalence_studies_properties_typeProperties_url": {
						"value": "[parameters('adls_Autism_prevalence_studies_properties_typeProperties_url')]"
					},
					"adls_National_Immunization_Survey_Adult_COVID_properties_typeProperties_url": {
						"value": "[parameters('adls_National_Immunization_Survey_Adult_COVID_properties_typeProperties_url')]"
					},
					"adls_Viral_hemorrhagic_fevers_properties_typeProperties_url": {
						"value": "[parameters('adls_Viral_hemorrhagic_fevers_properties_typeProperties_url')]"
					},
					"http_Autism_prevalence_studies_properties_typeProperties_url": {
						"value": "[parameters('http_Autism_prevalence_studies_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}